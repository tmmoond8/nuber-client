  },
  "devDependencies": {
    "@types/jest": "^24.0.12",
    "@types/node": "^11.13.8",
    "@types/react": "^16.8.15",
    "@types/react-dom": "^16.8.4",
    "@types/react-router-dom": "^4.3.2",
    "@types/react-sidebar": "^3.0.0",
    "@types/react-toastify": "^4.0.2",
    "typescript": "^3.4.5"
  }
}
